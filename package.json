  "scripts": {
    "asset-link": "npx react-native-asset",
    "start": "react-native start",
    "start:clean": "cd android && ./gradlew clean && cd .. && npm start -- --reset-cache",
    "clean": "npm cache clean --force && cd android && ./gradlew clean && cd ..",
    "android": "react-native run-android --active-arch-only --variant=devDebug --appIdSuffix=dev",
    "ios": "react-native run-ios --scheme \"Development\" --simulator=\"iPhone 14 Pro Max\"",
    "ios:dev": "react-native run-ios --scheme \"Development\" --simulator=\"iPhone 14 Pro Max\"",
    "ios:qat": "react-native run-ios --scheme \"QAT\" --simulator=\"iPhone 14 Pro Max\"",
    "ios:uat": "react-native run-ios --scheme \"UAT\" --simulator=\"iPhone 14 Pro Max\"",
    "ios:prod": "react-native run-ios --scheme \"Production\" --simulator=\"iPhone 14 Pro Max\"",
    "android:dev": "npx react-native run-android --active-arch-only --variant=devDebug --appIdSuffix=dev",
    "android:qat": "react-native run-android --active-arch-only --variant=qatDebug --appIdSuffix=qat",
    "android:uat": "react-native run-android --active-arch-only --variant=uatDebug --appIdSuffix=uat",
    "android:prod": "react-native run-android --active-arch-only --variant=prodDebug",
    "android:prodBundle": "react-native run-android --variant=prodrelease",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bundleApp": "cd android && ./gradlew bundleRelease"
  },
